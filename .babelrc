{
  "presets": [
    "@babel/env",
    "@babel/react",
    "@babel/typescript",
    "@emotion/css-prop"
  ],
  "plugins": [
    ["@babel/proposal-decorators", { "legacy": true }],
    ["@babel/proposal-class-properties", { "loose": true }],
    "@babel/transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    },
    "production": {
      "plugins": ["lodash"]
    }
  }
}
