{
  "presets": ["@babel/env", "@babel/react", "@babel/typescript"],
  "plugins": [
    ["@babel/proposal-decorators", { "legacy": true }],
    ["@babel/proposal-class-properties", { "loose": true }],
    "@babel/transform-runtime",
    "styled-components",
    "react-refresh/babel"
  ],
  "env": {
    "production": {
      "plugins": ["lodash"]
    }
  }
}
