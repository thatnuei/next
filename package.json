{
  "name": "next",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      ".",
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "build-scripts dev",
    "build": "build-scripts build",
    "test": "build-scripts test",
    "serve": "serve -s build",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck": "tsc --noEmit",
    "analyze": "yarn build && source-map-explorer build/static/js/**/*.js",
    "ci": "npm-run-all --parallel --print-label build test typecheck",
    "prepare": "typesync"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-next.12",
    "@mdi/js": "^5.0.45",
    "build-scripts": "*",
    "fchat": "^1.4.0",
    "focus-visible": "^5.0.2",
    "fuzzysearch": "^1.0.3",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "tailwindcss": "^1.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.8",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-window": "^1.8.1",
    "@types/testing-library__jest-dom": "^5.0.2",
    "@types/testing-library__react": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "preval.macro": "^5.0.0",
    "serve": "^11.3.0",
    "source-map-explorer": "^2.4.2",
    "twin.macro": "^1.0.0-alpha.8",
    "typescript": "^3.9.0-beta",
    "typescript-plugin-tw-template": "^1.0.2",
    "typesync": "^0.6.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules",
      "build"
    ]
  }
}
