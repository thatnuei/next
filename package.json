{
  "name": "next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "cross-env NODE_ENV=production craco build",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "serve": "serve -s build",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck": "tsc --noEmit",
    "analyze": "source-map-explorer build/static/js/**/*.js",
    "ci": "npm-run-all --parallel --print-label --continue-on-error build test typecheck",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@craco/craco": "5.6.4",
    "@emotion/babel-preset-css-prop": "11.0.0-next.10",
    "@emotion/react": "11.0.0-next.15",
    "@mdi/js": "5.5.55",
    "@seznam/compose-react-refs": "1.0.4",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "@types/lodash-es": "4.17.3",
    "@types/preval.macro": "3.0.0",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-window": "1.8.2",
    "@types/testing-library__jest-dom": "5.9.2",
    "cross-env": "7.0.2",
    "fake-indexeddb": "3.1.2",
    "fchat": "1.4.0",
    "focus-visible": "5.1.0",
    "framer-motion": "2.6.6",
    "fuzzysearch": "1.0.3",
    "idb": "5.0.4",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "lodash-es": "4.17.15",
    "micro-observables": "1.5.0",
    "npm-run-all": "4.1.5",
    "polished": "3.6.6",
    "prettier": "2.1.1",
    "preval.macro": "5.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-focus-lock": "2.4.1",
    "react-scripts": "3.4.3",
    "react-window": "1.8.5",
    "resize-observer-polyfill": "1.5.1",
    "serve": "11.3.2",
    "source-map-explorer": "2.5.0",
    "tailwindcss": "1.8.3",
    "twin.macro": "1.8.1",
    "typescript": "4.0.2",
    "typescript-plugin-tw-template": "2.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "quoteProps": "consistent",
    "trailingComma": "all"
  }
}
