{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "format": "prettier --write \"@(src|desktop|public|scripts)/**/*.@(js|ts|tsx|html|json|md)\"",
    "typecheck": "tsc --noEmit",
    "desktop:dev:app": "electron desktop/main.js",
    "desktop:dev": "npm-run-all --parallel --race desktop:dev:app dev",
    "desktop:build": "cross-env PUBLIC_URL=. yarn build",
    "desktop:start": "cross-env NODE_ENV=production electron desktop/main.js"
  },
  "dependencies": {
    "@craco/craco": "^5.5.0",
    "@mdi/js": "^4.5.95",
    "@mdi/react": "^1.2.1",
    "@reach/rect": "^0.3.0",
    "bbc.js": "https://github.com/kingdaro/bbc.js.git",
    "cross-env": "^6.0.3",
    "fchat": "^1.4.0",
    "focus-visible": "^5.0.2",
    "fuzzysearch": "^1.0.3",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.15",
    "mobx": "^5.14.0",
    "mobx-react-lite": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "polished": "^3.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-focus-on": "^3.1.5",
    "react-scripts": "^3.2.0",
    "react-transition-group": "^4.3.0",
    "react-window": "^1.8.5",
    "reakit": "^1.0.0-beta.4",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^4.4.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-transition-group": "^4.2.3",
    "@types/react-window": "^1.8.1",
    "@types/styled-components": "^4.1.19",
    "@types/uuid": "^3.4.5",
    "@types/webpack-env": "^1.14.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.10.6",
    "electron": "^6.0.12",
    "prettier": "^1.18.2",
    "typescript": "3.7.0-dev.20191011",
    "typescript-styled-plugin": "^0.14.0"
  },
  "sideEffects": [
    "focus-visible"
  ],
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
