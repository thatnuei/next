{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "format": "prettier --write \"@(src|desktop|public|scripts)/**/*.@(js|ts|tsx|html|json|md)\"",
    "typecheck": "tsc --noEmit",
    "desktop:dev:app": "electron desktop/main.js",
    "desktop:dev": "npm-run-all --parallel --race desktop:dev:app dev",
    "desktop:build": "cross-env PUBLIC_URL=. yarn build",
    "desktop:start": "cross-env NODE_ENV=production electron desktop/main.js",
    "postinstall": "typesync",
    "postuninstall": "typesync"
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@mdi/js": "^4.7.95",
    "@mdi/react": "^1.2.1",
    "@reach/rect": "^0.7.2",
    "bbc.js": "https://github.com/kingdaro/bbc.js.git",
    "cross-env": "^6.0.3",
    "fchat": "^1.4.0",
    "focus-visible": "^5.0.2",
    "fuzzysearch": "^1.0.3",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.2",
    "mobx-react-lite": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-focus-on": "^3.3.0",
    "react-scripts": "^3.3.0",
    "react-transition-group": "^4.3.0",
    "react-window": "^1.8.5",
    "reakit": "^1.0.0-beta.4",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.6",
    "@types/prettier": "~1.19.0",
    "@types/reach__rect": "^0.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-transition-group": "^4.2.3",
    "@types/react-window": "^1.8.1",
    "@types/styled-components": "^4.4.2",
    "@types/testing-library__react": "^9.1.2",
    "@types/uuid": "^3.4.6",
    "@types/webpack-env": "^1.15.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.10.6",
    "electron": "^7.1.9",
    "prettier": "~1.19.1",
    "tailwindcss": "^1.1.4",
    "typescript": "~3.7.4",
    "typescript-styled-plugin": "^0.15.0",
    "typesync": "^0.6.1"
  },
  "sideEffects": [
    "focus-visible"
  ],
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
