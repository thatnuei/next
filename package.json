{
  "name": "next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "serve": "serve -s build",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck": "tsc --noEmit",
    "analyze": "yarn build && source-map-explorer build/static/js/**/*.js",
    "ci": "npm-run-all --parallel --print-label build test typecheck",
    "postinstall": "typesync"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-next.11",
    "@emotion/serialize": "^0.12.0-next.3",
    "@mdi/js": "^4.9.95",
    "fchat": "^1.4.0",
    "focus-visible": "^5.0.2",
    "idb-keyval": "^3.2.0",
    "polished": "^3.4.4",
    "preact": "^10.3.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@craco/craco": "^5.6.3",
    "@emotion/babel-preset-css-prop": "^11.0.0-next.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.2.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/babel__core": "^7.1.6",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/testing-library__jest-dom": "^5.0.2",
    "@types/testing-library__react": "^9.1.3",
    "@types/webpack-hot-middleware": "^2.25.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "react-refresh": "^0.8.0",
    "react-scripts": "^3.4.0",
    "serve": "^11.3.0",
    "source-map-explorer": "^2.3.1",
    "typescript": "~3.8.3",
    "typesync": "^0.6.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules",
      "build"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
