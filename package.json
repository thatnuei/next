{
  "name": "next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "serve": "serve -s build",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck": "tsc --noEmit",
    "analyze": "yarn build && source-map-explorer build/static/js/**/*.js",
    "ci": "npm-run-all --parallel --print-label build test typecheck",
    "postinstall": "typesync",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-next.12",
    "@mdi/js": "^5.0.45",
    "fchat": "^1.4.0",
    "focus-visible": "^5.1.0",
    "fuzzysearch": "^1.0.3",
    "idb": "^5.0.2",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "polished": "^3.5.1",
    "preact": "^10.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-focus-on": "^3.3.0",
    "react-window": "^1.8.5",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@emotion/babel-preset-css-prop": "^11.0.0-next.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.2.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/babel__core": "^7.1.6",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@types/prettier": "^2.0.0",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-window": "^1.8.1",
    "@types/testing-library__jest-dom": "^5.0.2",
    "@types/testing-library__react": "^10.0.0",
    "@types/webpack-hot-middleware": "^2.25.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "fake-indexeddb": "^3.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "preval.macro": "^5.0.0",
    "react-refresh": "^0.8.1",
    "react-scripts": "^3.4.1",
    "serve": "^11.3.0",
    "source-map-explorer": "^2.4.2",
    "twin.macro": "^1.0.0-alpha.8",
    "typescript": "^3.9.0-beta",
    "typescript-plugin-tw-template": "^1.0.2",
    "typesync": "^0.6.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules",
      "build"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "quoteProps": "consistent",
    "trailingComma": "all"
  }
}
