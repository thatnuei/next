	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">1%",
			"not dead",
			"not ie <= 11",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"quoteProps": "consistent",
		"semi": false,
		"trailingComma": "all",
		"useTabs": true
	}
